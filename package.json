{
  "name": "js-bootstrapper",
  "version": "1.0.0",
  "description": "What if you dont need to install yourself all those small tools manually each time you bootstrap a project?",
  "dependencies": {
    "axios": "^0.21.1",
    "inquirer": "^8.0.0"
  },
  "bin": {
    "js-boot": "dist/ui-options.js"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.32",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "start": "ts-node src/ui-options.ts",
    "format": "prettier --write \"**/*.{json,md,ts,tsx,yml,js,jsx}\"",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayonious/js-bootstrapper.git"
  },
  "keywords": [],
